<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC 
	"-//Apache Software Foudation//DTD Struts Configration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
	
<struts>
	<package name="AccountAction"  extends="struts-default" namespace="/">
		<action name="rechargeAction" class="AccountAction"  method="rechargeAction">
			<result name="success">/WEB-INF/jsp/index.jsp</result>
			<result name="failed">/WEB-INF/jsp/index.jsp</result>
		</action>
		<action name="payAction" class="AccountAction"  method="payAction">
			<result name="success">/WEB-INF/jsp/index.jsp</result>
			<result name="failed">/WEB-INF/jsp/index.jsp</result>
		</action>
	</package>
</struts> 