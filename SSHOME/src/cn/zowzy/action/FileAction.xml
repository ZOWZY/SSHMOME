<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC 
	"-//Apache Software Foudation//DTD Struts Configration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
	
<struts>
	<package name="FileAction"  extends="struts-default" namespace="/">
		<action name="photoUpload" class="FileAction"  method="photoUpload">
			<result name="success">/WEB-INF/jsp/photoUpload.jsp</result>
			<result name="failed">/WEB-INF/jsp/failed.jsp</result>
		</action>
		<action name="photoUploadPage" class="FileAction"  method="photoUploadPage">
			<result name="success">/WEB-INF/jsp/photoUpload.jsp</result>
			<result name="failed">/WEB-INF/jsp/failed.jsp</result>
		</action>
	</package>
</struts> 