<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC 
	"-//Apache Software Foudation//DTD Struts Configration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
	
<struts>
	<package name="IndexAction"  extends="struts-default" namespace="/">
		<action name="indexPage" class="IndexAction"  method="indexPage">
			<result name="success">/WEB-INF/jsp/userMain.jsp</result>
		</action>
		
		<action name="imgCarouselPageAction" class="IndexAction"  method="imgCarouselPageAction">
			<result name="success">/WEB-INF/jsp/imgcarousel.jsp</result>
		</action>
		
		<action name="userMyPageAction" class="IndexAction"  method="userMyPageAction">
			<result name="success">/WEB-INF/jsp/userMy.jsp</result>
		</action>
	</package>
</struts> 