<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC 
	"-//Apache Software Foudation//DTD Struts Configration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<package name="UsersCenterAction"  extends="struts-default"  namespace="/">
	
		<action name="userInfo" class="UsersCenterAction" method="userInfo">
			<result name="failed">/WEB-INF/jsp/userMain.jsp</result>
			<result name="success">/WEB-INF/jsp/userInfor.jsp</result>
		</action>
		
		<action name="userMyHome" class="UsersCenterAction" method="userMyHome">
			<result name="failed">/WEB-INF/jsp/userMain.jsp</result>
			<result name="success">/WEB-INF/jsp/userMyHome.jsp</result>
		</action>
		
	
	
	</package>

</struts>